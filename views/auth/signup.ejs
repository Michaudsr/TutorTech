<form action="/auth/signup" method="POST">
  <label for="first-name">First name</label>
  <input id="first-name" type="text" name="name">
  
  <label for="Last-name">Last name</label>
  <input id="Last-name" type="text" name="name">

  <label for="new-email">Email</label>
  <input id="new-email" type="email" name="email">
  
  <label for="new-password">Password</label>
  <input id="new-password" type="password" name="password">

  <label for="city">City</label>
  <input id="city" type="city" name="city">

  <label for="state">State</label>
  <input id="state" type="state" name="state">

  <label for="description">Description</label>
  <input id="description" type="description" name="description">


  <input type="submit" value="Sign up">
</form>


